{
  "title": "Tus.io",
  "name": "tus-io",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "inject:community": "_scripts/inject-community.sh",
    "inject:protocol": "_scripts/inject-protocol.sh",
    "inject": "npm-run-all --parallel inject:*",
    "deploy": "lanyon deploy",
    "slackin:deploy": "source env.sh && _scripts/slackin.sh",
    "download:logos": "cross-env DEBUG=*:* IMAGES_CFG=_data/logos.yml IMAGES_DIR=assets/img/logos node ./_scripts/download-external-images.js",
    "download:press": "cross-env DEBUG=*:* IMAGES_CFG=_data/press.yml IMAGES_DIR=assets/img/press node ./_scripts/download-external-images.js",
    "optimize:logos": "cross-env IMAGES_CFG=_data/logos.yml IMAGES_DIR=assets/img/logos _scripts/images-optimize.sh",
    "optimize:press": "cross-env IMAGES_CFG=_data/press.yml IMAGES_DIR=assets/img/press _scripts/images-optimize.sh",
    "pull": "git pull",
    "build:assets:production": "LANYON_ENV=production lanyon build:assets",
    "build:production": "LANYON_ENV=production lanyon build",
    "build": "lanyon build",
    "serve:production": "LANYON_ENV=production lanyon serve",
    "serve": "lanyon serve",
    "encrypt": "lanyon encrypt",
    "start:production": "npm run build:production && npm run serve:production",
    "upgrade:modules": "npm-check-updates -u --semverLevel major && yarn",
    "start": "lanyon start",
    "lint": "eslint .",
    "fix": "eslint . --fix"
  },
  "devDependencies": {
    "animate.css": "3.2.5",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-es6-promise": "1.1.1",
    "babel-plugin-syntax-async-functions": "6.13.0",
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-plugin-transform-object-assign": "6.22.0",
    "babel-preset-es2015": "6.24.1",
    "cross-env": "2.0.1",
    "depurar": "0.3.0",
    "download": "5.0.3",
    "es6-promise": "4.1.1",
    "eslint": "3.19.0",
    "eslint-config-standard": "6.2.1",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-standard": "2.3.1",
    "fakefile": "0.0.9",
    "glob": "7.1.2",
    "jquery": "2.2.0",
    "js-yaml": "3.13.1",
    "lanyon": "0.0.142",
    "npm-check-updates": "2.12.1",
    "npm-run-all": "2.3.0",
    "on-the-githubs": "2.4.0",
    "skeleton-css": "2.0.4",
    "stylelint": "7.13.0",
    "timeago": "1.4.1",
    "tus-js-client": "1.4.4"
  },
  "engines": {
    "yarn": ">= 1.0.0"
  }
}
