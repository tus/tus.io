{
  "title": "Tus.io",
  "name": "tus-io",
  "version": "0.0.1",
  "files": [],
  "dependencies": {
    "autoprefixer": "6.0.3",
    "babel": "5.8.23",
    "clean-css": "3.4.5",
    "concat-from-list": "0.1.2",
    "depurar": "^0.2.0",
    "download": "^4.2.1",
    "glob": "5.0.15",
    "js-yaml": "^3.4.3",
    "jshint": "2.8.0",
    "less": "2.5.3",
    "on-the-githubs": "2.3.0",
    "postcss": "5.0.9",
    "postcss-cli": "2.3.1",
    "postcss-reporter": "1.2.1",
    "run-scripts": "0.4.0",
    "stylelint": "1.2.1",
    "tus-js-client": "1.0.0",
    "uglify-js": "2.4.24",
    "watch": "0.16.0"
  },
  "scripts": {
    "js:concat": "concat-from-list _assets/app.json assets/build/app.js",
    "js:minify": "uglifyjs assets/build/app.js -o assets/build/app.js",
    "js:minify:source": "uglifyjs assets/build/app.js -o assets/build/app.js --source-map assets/build/app.js.map",
    "js:hint": "jshint $(find ./_assets/js -name \"*.js\") ./_scripts/*.js",
    "js": "run-scripts js:hint js:concat js:minify",
    "css:less": "lessc _assets/app.less > assets/build/app.css",
    "css:prefix": "postcss --use autoprefixer assets/build/app.css",
    "css:minify": "cleancss -o ./assets/build/app.css ./assets/build/app.css",
    "css:lint": "babel-node _scripts/stylelint.js ./_assets/\\*\\*/\\*.less",
    "css": "run-scripts css:less css:prefix css:minify",
    "build": "run-scripts css js",
    "build:watch": "watch \"npm run build\" ./_assets",
    "server": "jekyll serve"
  }
}
