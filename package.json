{
  "title": "Tus.io",
  "name": "tus-io",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "build:assets:production": "LANYON_ENV=production LANYON_JEKYLL=${PWD}/_jekyll/jekyll.sh lanyon build:assets",
    "build:production": "LANYON_ENV=production LANYON_JEKYLL=${PWD}/_jekyll/jekyll.sh lanyon build",
    "build": "LANYON_JEKYLL=${PWD}/_jekyll/jekyll.sh lanyon build",
    "deploy": "LANYON_JEKYLL=${PWD}/_jekyll/jekyll.sh lanyon deploy",
    "download:logos": "cross-env DEBUG=*:* IMAGES_CFG=_data/logos.yml IMAGES_DIR=assets/img/logos node ./_scripts/download-external-images.js",
    "download:press": "cross-env DEBUG=*:* IMAGES_CFG=_data/press.yml IMAGES_DIR=assets/img/press node ./_scripts/download-external-images.js",
    "fix": "eslint . --fix",
    "inject:community": "_scripts/inject-community.sh",
    "inject:protocol": "_scripts/inject-protocol.sh",
    "inject": "npm-run-all --parallel inject:*",
    "lint": "eslint .",
    "optimize:logos": "cross-env IMAGES_CFG=_data/logos.yml IMAGES_DIR=assets/img/logos _scripts/images-optimize.sh",
    "optimize:press": "cross-env IMAGES_CFG=_data/press.yml IMAGES_DIR=assets/img/press _scripts/images-optimize.sh",
    "pull": "git pull",
    "serve:production": "LANYON_ENV=production LANYON_JEKYLL=${PWD}/_jekyll/jekyll.sh lanyon serve",
    "serve": "LANYON_JEKYLL=${PWD}/_jekyll/jekyll.sh lanyon serve",
    "start:production": "npm run build:production && npm run serve:production",
    "start": "LANYON_JEKYLL=${PWD}/_jekyll/jekyll.sh lanyon start",
    "test": "true",
    "upgrade:modules": "updtr"
  },
  "devDependencies": {
    "cross-env": "7.0.3",
    "depurar": "0.3.0",
    "download": "8.0.0",
    "eslint": "8.32.0",
    "eslint-config-standard": "16.0.3",
    "eslint-import-resolver-webpack": "0.13.2",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prefer-import": "0.0.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.32.1",
    "eslint-plugin-standard": "5.0.0",
    "fakefile": "1.1.0",
    "glob": "8.1.0",
    "jquery": "3.6.3",
    "js-yaml": "4.1.0",
    "lanyon": "0.3.33",
    "npm-run-all": "4.1.5",
    "on-the-githubs": "2.4.0",
    "prismjs": "1.29.0",
    "regenerator-runtime": "0.13.11",
    "s-ago": "2.2.0",
    "skeleton-css": "2.0.4",
    "timeago": "1.6.7",
    "tus-js-client": "3"
  },
  "engines": {
    "yarn": ">= 1.0.0"
  },
  "dependencies": {}
}
